{
  "manifest_version": 2,
  "name": "Firebase Auth in Chrome Extension Sample",
  "description": "This sample shows how to authorize Firebase in a Chrome extension using a Google account.",
  "version": "0.1",
  "permissions": [
    "identity"
  ],
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "content_security_policy":"script-src 'self' https://www.gstatic.com/ https://*.firebaseio.com https://*.googleapis.com https://apis.google.com; object-src 'self'",
  "oauth2": {
    "client_id": "732832856046-ko27pdudmuu8ah2fho31cd90i61sig1b.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile",
      "https://www.googleapis.com/auth/calendar.readonly",
      "https://www.googleapis.com/auth/calendar"
    ]
  },
  "key": "-----BEGIN PUBLIC KEY----------END PUBLIC KEY-----"
}
